{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://slsp/schema/drawdown-request.schema.json",
  "title": "Drawdown Request",
  "type": "object",
  "required": [
    "drawdown"
  ],
  "properties": {
    "drawdown": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "invoiceNumber": {
            "type": "string"
          },
          "variableSymbol": {
            "type": "string"
          },
          "iban": {
            "type": "string"
          },
          "amount": {
            "type": "number"
          },
          "note": {
            "type": "string"
          },
          "currency": {
            "type": "string",
            "default": "EUR"
          }
        }
      }
    }
  }
}